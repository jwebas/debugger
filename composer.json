{
    "name" : "jwebas/debugger",
    "description" : "Slim Framework Tracy Debugger Bar",
    "license" : "MIT",
    "type" : "library",
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "authors" : [
        {
            "name" : "Martynas",
            "email" : "jwebas@gmail.com"
        }
    ],
    "require" : {
        "php" : ">=7.1.3",
        "psr/container" : "^1.0",
        "tracy/tracy" : "^2.5"
    },
    "require-dev" : {
        "illuminate/database" : "^5.7",
        "jwebas/config" : "dev-master",
        "slim/slim" : "^3.12",
        "slim/twig-view" : "^2.4"
    },
    "autoload" : {
        "psr-4" : {
            "Jwb\\" : "src"
        }
    },
    "config" : {
        "optimize-autoloader" : true,
        "sort-packages" : true
    },
    "repositories" : [
        {
            "type" : "vcs",
            "url" : "git@github.com:jwebas/config.git"
        },
        {
            "type" : "vcs",
            "url" : "git@github.com:jwebas/utils.git"
        }
    ]
}
